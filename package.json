{
  "name": "vnaver",
  "displayName": "VNAVer",
  "version": "0.14.0",
  "description": "A simple application to create VNAV diagrams.",
  "author": {
    "email": "anickle060193@gmail.com",
    "name": "Adam Nickle"
  },
  "private": true,
  "homepage": "./",
  "main": "./public/electron.js",
  "build": {
    "appId": "com.adamnickle.vnaver",
    "productName": "VNAVer",
    "win": {
      "icon": "./build/icon.ico",
      "publish": "github"
    },
    "nsis": {
      "perMachine": true,
      "createDesktopShortcut": false
    },
    "directories": {
      "buildResources": "public"
    },
    "fileAssociations": {
      "ext": "vnav",
      "name": "VNAV",
      "description": "VNAVer Document"
    }
  },
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"REACT,ELECTRON\" -c \"magenta.bold,blue.bold\"  \"yarn run react-start\" \"wait-on http-get://localhost:3000/ && yarn run electron-start\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "cross-env NODE_ENV=development electron .",
    "react-start": "cross-env NODE_ENV=development react-scripts start",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && electron-builder",
    "release": "yarn run build && env-cmd ./secret.env electron-builder --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": "electron >= 7",
  "dependencies": {
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@types/classnames": "^2.2.9",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux-logger": "^3.0.7",
    "@types/uuid": "^3.4.6",
    "@types/webpack-env": "^1.14.1",
    "ajv": "^6.10.2",
    "ajv-keywords": "^3.4.1",
    "bootstrap-css-only": "^4.3.1",
    "classnames": "^2.2.6",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "electron-log": "^3.0.9",
    "electron-updater": "^4.2.0",
    "konva": "^4.0.18",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-konva": "^16.10.1-0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-undo": "^1.0.0-beta9-9-7",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typeface-roboto": "^0.0.75",
    "typeface-roboto-mono": "^0.0.75",
    "typescript": "3.7.2",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "uuid": "^3.3.3",
    "wait-on": "^3.3.0"
  }
}
